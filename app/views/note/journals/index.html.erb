<div>
	<table class="table table-hover">
		<thead>
			<tr>
				<th>创建者</th>
				<th>标题</th>
				<th></th>
			</tr>
		</thead>
		<tbody>
			<% @journals.each do |journal| %>
				<tr>
					<td><%= journal.user_name%></td>
					<td><%= journal.title%></td>
					<td><%= link_to "show", note_journal_path(journal) %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
</div>