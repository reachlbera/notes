<div class="main-data">
	pictures<%= @file_url%>
	<div class="" style="position:fixed;top:0px;right:0px;">
		<a href="/<%= params[:controller]%>/new" class="btn btn-default">新增</a>
	</div>
	<% if @pictures%>
	<% @pictures.each do |file| %>
		<div class="form-group" style="display:table;width:100%;">
			<div class="col-xs-3"><img src="/assets/<%= file.content_type+"/"+file.disk_filename%>" width="120" height="120"></div>
			<div class="col-xs-3"><%= file.filename%></div>
			<div class="col-xs-3"><%= file.content_type%></div>
			<div class="col-xs-3"><%= file.disk_filename%></div>
		</div>
	<% end %>
	<% end %>
</div>